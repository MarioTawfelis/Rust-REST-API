POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=firefleeb

# Used when running `cargo run` locally (DB host is localhost)
DATABASE_URL=postgres://postgres:password@localhost:5432/firefleeb

# Used by docker-compose (DB host is the service name `db`)
DOCKER_DATABASE_URL=postgres://postgres:password@db:5432/firefleeb

PORT=8080
RUST_LOG=info
